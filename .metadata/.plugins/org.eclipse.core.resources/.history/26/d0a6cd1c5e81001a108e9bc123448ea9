package util;

public class Coord<T, A> {

	private final int x;
	private final int y;

	public Coord(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}

	public boolean equals(Coord<?, ?> o) {
			return this.x == o.getX() && this.y == o.getY();
	}

}