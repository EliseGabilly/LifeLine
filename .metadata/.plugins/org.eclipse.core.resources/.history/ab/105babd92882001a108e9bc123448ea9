package util;

import java.util.Comparator;

import obj.Path;

public class NodeComparator implements Comparator<Integer> {

	@Override
	public int compare(int n1, int n2) {
		if (p1.getPathHeuristic() < p2.getPathHeuristic())
			return 1;
		else if (p1.getPathHeuristic() > p2.getPathHeuristic())
			return -1;
		return 0;
	}
}