package util;

import java.util.Comparator;

import obj.Path;

public class NodeComparator implements Comparator<Path> {

	@Override
	public int compare(Path p1, Path p2) {
		if (p1.getPathHeuristic() < p2.getPathHeuristic())
			return 1;
		else if (p1.getPathHeuristic() > p2.getPathHeuristic())
			return -1;
		return 0;
	}
}