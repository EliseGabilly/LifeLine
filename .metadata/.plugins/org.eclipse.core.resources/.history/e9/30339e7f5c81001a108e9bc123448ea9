package util;

public class Coord<T, T> {

	private final int x;
	private final int y;

	public Coord(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}

	public int hashCode() {
		return x.hashCode() ^ right.hashCode();
	}

	public boolean equals(Object o) {
		if (!(o instanceof Coord))
			return false;
		Coord pairo = (Coord) o;
		return this.left.equals(pairo.getLeft()) && this.right.equals(pairo.getRight());
	}

}